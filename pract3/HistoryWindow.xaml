<Window x:Name="histwindow" x:Class="pract3.HistoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pract3"
        mc:Ignorable="d"
        Title="История прослушивания" Height="450" Width="450">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="38*"/>
            <ColumnDefinition Width="37*"/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="hist" d:ItemsSource="{d:SampleData ItemCount=5}" SelectionChanged="hist_SelectionChanged" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
